    <eStructuralFeatures xsi:type="ecore:EReference" name="stmtBlock" eType="#//StmtBlock"
        containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Event" eSuperTypes="#//EventBlock">
